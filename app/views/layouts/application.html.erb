<!DOCTYPE html>
<html>
  <head>
    <title>EuroVintVoting</title>
    <%= csrf_meta_tags %>
    <%= javascript_include_tag :application %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
  </head>

  <body>

    <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>

    <%= yield %>
